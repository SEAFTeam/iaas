    {{#items}}
        {{entity}}.{{metadata.uid}}:
            id: {{metadata.uid}}
            title: {{metadata.name}}
            type: {{spec.type}}
            namespace_id: {{parent.entity}}.{{parent.item.metadata.uid}}
            ports:
            {{#spec.ports}}
                - name: {{name}}
                  port: {{port}}
                  target_port: {{target_port}}
                  node_port: {{node_port}}
                  protocol: {{protocol}}
            {{/spec.ports}}
            {{#spec.selector}}
            selector:
            {{#iterator}}
                @kv{{{.}}}
            {{/iterator}}
            {{/spec.selector}}
            {{#metadata.labels}}
            labels:
                {{#iterator}}
                @kv{{{.}}}
                {{/iterator}}
            {{/metadata.labels}}
    {{/items}}
