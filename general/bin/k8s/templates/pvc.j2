    {% for pvc in items %}
    {{domain}}.k8s.pvc.{{pvc.metadata.uid}}:
        id: {{domain}}.k8s.pvc.{{pvc.metadata.uid}}
        title: {{pvc.metadata.name}}
        namespace_id: {{domain}}.k8s.namespace.{{parent.item.metadata.uid}}
        volume_name: {{pvc.spec.volume_name}}
        volume_mode: {{pvc.spec.volume_mode}}
        capacity: {{pvc.status.capacity.storage}}{% endfor %}
